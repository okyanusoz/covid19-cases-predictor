{
    "$id": "data.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Representation of predicted and current total cases and deaths",
    "type": "object",
    "required": [
        "prediction",
        "current"
    ],
    "properties": {
        "prediction": {
            "description": "Predicted total cases and deaths",
            "$ref": "data-state.schema.json"
        },
        "current": {
            "description": "Current total cases and deaths",
            "$ref": "data-state.schema.json"
        }
    }
}
